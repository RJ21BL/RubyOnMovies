<h1>Movies</h1>
<% @movies.each do |movie| %>
  <%= movie.id %>
  <%= link_to movie.title, movie %>
  <%= movie.year %>
    <p>
  <strong><%= link_to 'DELETE', movie, method: :delete  %> </strong>
    <p>
  <strong><%= link_to 'EDIT', edit_movie_path(movie) %></strong>
    <p>
<% end %>

<h2><%= link_to 'Add A Movie', new_movie_path %></h2>
